<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
     <%
     //form에서 보내준 데이터 받아오기
     
     String id = request.getParameter("id");
     String pw = request.getParameter("pw");
     String nickname = request.getParameter("nickname");
     	System.out.println(id);
    	System.out.println(pw);
     	System.out.println(nickname);
     
     
     session.setAttribute("session_id", "kkk1234");
     session.setAttribute("session_pw", "1111");
     session.setAttribute("session_nick", "");
     
     if(id.equals("kkk1234") && pw.equals("1111")){
     	// 쿠키 발급 여부와 상관없이 로그인에 성공했으므로 웰컴페이지로 이동.
     	response.sendRedirect("session_welcom.jsp");
     }
     
     
 	 %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<h2>로그인에 실패했습니다.</h2>
	<a href="session_login.jsp">로그인화면으로 돌아가기</a>
</body>
